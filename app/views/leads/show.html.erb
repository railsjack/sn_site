<div class="row">
    <div class="col-md-8 col-md-offset-2">
    <div class="panel panel-success">
      <div class="panel-heading">
        <h3 class="panel-title">Lead</h3>
      </div>
      <div class="panel-body">
          
<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @lead.name %>
</p>

<p>
  <strong>Email:</strong>
  <%= @lead.email %>
</p>

          
<p>
  <strong>Created By:</strong>
  <%= @lead.profile.user.name if  @lead.profile && @lead.profile.user%>
</p>

</div>
     <div class="panel-footer">
        <%= link_to 'Edit', edit_profile_lead_path(@lead.profile, @lead) , class: 'btn btn-success btn-md'%> |
        <%= link_to 'Back', profile_leads_path , class: 'btn btn-success btn-md'%>         
    </div>        
    </div>
    </div>    
</div>            